{
	"version": 6,
	"configurePresets": [
		{
			"name": "windows",
			"hidden": true,
			"cacheVariables": {
				"YABOC_PLATFORM": {
					"type": "STRING",
					"value": "YABOC_PLATFORM_WINDOWS"
				}
			},
			"condition": {
				"type": "equals",
				"lhs": "${hostSystemName}",
				"rhs": "Windows"
			}
		},
		{
			"name": "linux",
			"hidden": true,
			"cacheVariables": {
				"YABOC_PLATFORM": {
					"type": "STRING",
					"value": "YABOC_PLATFORM_WINDOWS"
				}
			},
			"condition": {
				"type": "equals",
				"lhs": "${hostSystemName}",
				"rhs": "Linux"
			}
		}
	]
}
